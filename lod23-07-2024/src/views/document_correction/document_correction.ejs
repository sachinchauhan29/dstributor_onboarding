      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:../../partials/_sidebar.html -->
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
          <ul class="nav">
            <li class="nav-item">
              <a class="nav-link" href="/userrole">
                <i class="mdi mdi-home menu-icon"></i>
                <span class="menu-title">Details & Document Upload</span>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/doc_correction">
                <i class="mdi mdi-library-books menu-icon"></i>
                <span class="menu-title">Document Correction</span>
              </a>
            </li>
          </ul>
        </nav>
        <!-- partial -->
        <div class="main-panel role-user-status">
          <div class="content-wrapper">
            <div class="row">
              <div class="col-12 grid-margin stretch-card">
                <div class="card p-3 table-responsive">
                  <div class="document-correction">
                    <div
                      class="d-flex mb-md-5 heading-text justify-content-center py-4"
                    >
                      <span class="position-relative">Document Correction</span>
                    </div>
                    <div class="row g-3 align-items-center">
                      <div class="col-md-6 form-group">
                        <label>Document to be Corrected :</label>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <input
                            type="text"
                            class="form-control"
                            name="father_name"
                            id="father_name"
                            placeholder="Document Name"
                          />
                        </div>
                      </div>
                    </div>
                    <div class="row g-3 align-items-center">
                      <div class="col-md-6 form-group">
                        <label>Remarks For Correction :</label>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <input
                            type="text"
                            class="form-control"
                            name="father_name"
                            id="father_name"
                            placeholder="Remarks Here"
                          />
                        </div>
                      </div>
                    </div>
                    <div class="row g-3 align-items-center">
                      <div class="col-md-6 form-group">
                        <label>Document Reupload :</label>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group mb-2">
                          <input
                            type="file"
                            class="form-control"
                            id="images"
                            name="upload_aadhar"
                          />
                        </div>
                        <span class="extinguisher_img align-items-center">
                          Accepted Formats:
                          <ul class="d-flex align-items-center p-0 m-0">
                            <li>.FG</li>
                            <li>JPEG</li>
                            <li>PNG</li>
                            <li>PDF</li>
                          </ul>
                        </span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- content-wrapper ends -->
          <!-- partial:../../partials/_footer.html -->
          <footer class="footer">
            <div class="container-fluid text-center">
              <span
