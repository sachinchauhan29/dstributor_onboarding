                }
                return isValid;
            }

            validateAndToggleNextButton();

            $('#fieldset-2 input, #fieldset-2 textarea, #fieldset-2 select').on('input change', function () {
                validateAndToggleNextButton();
            });

            $('#fieldset-2 input[type="radio"]').on('change', function () {
                validateAndToggleNextButton();
            });

            $('#images').on('change', function () {
                validateAndToggleNextButton();
            });

            function validateAndToggleNextButton() {
                var isValid = validateFieldset2();

                if (isValid) {
                    $('#fieldset-2 .form-wizard-next-btn').show();
                } else {
                    $('#fieldset-2 .form-wizard-next-btn').hide();
                }
            }
        });
    </script>


    <script>
        $(document).ready(function () {
            function validateFieldset3() {
                var isValid = true;

                if (!$('input[name="radio-save-future"]:checked').val()) {
                    isValid = false;
                }

                var email = $('#distributor_email_id').val().trim();
                var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                if (!emailRegex.test(email)) {
                    isValid = false;
                }

                return isValid;
            }

            validateAndToggleSubmitButton();

            $('#fieldset-3 input, #fieldset-3 textarea').on('input change', function () {
                validateAndToggleSubmitButton();
            });

            $('#fieldset-3 input[type="radio"]').on('change', function () {
                validateAndToggleSubmitButton();
            });

            $('#fieldset-3').on('submit', function (event) {
                if (!validateFieldset3()) {
                    event.preventDefault();
                }
            });

            function validateAndToggleSubmitButton() {
                var isValid = validateFieldset3();

                if (isValid) {
                    $('#fieldset-3 .form-wizard-next-btn').prop('disabled', false);
                } else {
                    $('#fieldset-3 .form-wizard-next-btn').prop('disabled', true);
                }
            }
        });
    </script>

    <script>
        document.getElementById('mobile_number').addEventListener('input', function () {
            let mobileNumber = this.value.replace(/[^\d]/g, '');
            if (mobileNumber.length !== 10) {
                this.setCustomValidity('Mobile number must be exactly 10 digits long.');
            } else {
                this.setCustomValidity('');
            }
        });
    </script>

