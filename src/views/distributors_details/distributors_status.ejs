    <script>

        $(document).ready(function () {

            function validateFields() {
                var isValid = true;

                if (!$('input[name="radio-name1"]:checked').val()) {
                    isValid = false;
                }

                var gstNumber = $('#gst_number').val().trim();
                if (gstNumber === '') {
                    isValid = false;
                } else {
                    var gstValue = gstNumber.toUpperCase();
                    var gstPattern = /^([0-9]{2}[a-zA-Z]{4}([a-zA-Z]{1}|[0-9]{1})[0-9]{4}[a-zA-Z]{1}([a-zA-Z]|[0-9]){3}){0,15}$/;
                    if (!gstPattern.test(gstValue)) {
                        isValid = false;
                    }
                }

                if (!$('input[name="radio-name"]:checked').val()) {
                    isValid = false;
                }

                var numberPartner = $('#number_partner').val().trim();
                if (numberPartner === '' || isNaN(numberPartner)) {
                    isValid = false;
                }

                if ($('#Personal_details').val().trim() === '' || $('#mobile_number').val().trim() === '') {
                    isValid = false;
                } else {
                    var mobileNumber = $('#mobile_number').val().trim();
                    var mobileNumberPattern = /^\d{10}$/;
                    if (!mobileNumberPattern.test(mobileNumber)) {
                        //   alert('Please ensure all personal details are filled out correctly and the mobile number is exactly 10 digits long.');
                        isValid = false;
                    }
                }

                // if (!isValid) {
                //     alert('Please ensure all personal details are filled out correctly and the mobile number is exactly 10 digits long.');
                // }


                if ($('select[name="department"]').val().trim() === '') {
                    isValid = false;
                }

                if ($('select[name="dpeartment_fun"]').val().trim() === '') {
                    isValid = false;
                }


                if (isValid) {
                    $('.next-btn').removeClass('disabled-link');
                } else {
                    $('.next-btn').addClass('disabled-link');
                }
            }

            $('input, select').on('input change', function () {
                validateFields();
            });

            validateFields();
        });
    </script>



