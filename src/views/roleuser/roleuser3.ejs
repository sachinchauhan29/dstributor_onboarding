    <script>
      document.getElementById("tab-investor");
      document
        .getElementById("tab-distributor")
        .addEventListener("click", function () {
          document.getElementById("distributor-fieldset").classList.add("show");
          for (let i = 0; i < 3; i++) {
            document
              .getElementById(`investor-fieldset-${i}`)
              .classList.remove("show");
          }
          this.classList.add("active");
          document.getElementById("tab-investor").classList.remove("active");
        });

      for (let i = 0; i < 3; i++) {
        document
          .getElementById(`tab-investor-${i}`)
          .addEventListener("click", function () {
            hideAllFieldsets();
            document
              .getElementById(`investor-fieldset-${i}`)
              .classList.add("show");
            this.classList.add("active");
          });
      }

      function hideAllFieldsets() {
        document
          .getElementById("distributor-fieldset")
          .classList.remove("show");
        for (let i = 0; i < 3; i++) {
          document
            .getElementById(`investor-fieldset-${i}`)
            .classList.remove("show");
        }
        document.getElementById("tab-distributor").classList.remove("active");
        for (let i = 0; i < 3; i++) {
          document
            .getElementById(`tab-investor-${i}`)
            .classList.remove("active");
        }
      }
    </script>

