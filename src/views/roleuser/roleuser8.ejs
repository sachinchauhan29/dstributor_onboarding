    <script>
      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("Fixed_Assests")
          .addEventListener("input", function () {
            const numberOfVehicles = parseInt(this.value, 10);
            const tableBody = document.getElementById("asset_fieldstableBody");
            tableBody.innerHTML = "";

            if (isNaN(numberOfVehicles) || numberOfVehicles <= 0) {
              return;
            }

            for (let i = 0; i < numberOfVehicles; i++) {
              const row = document.createElement("tr");

              const rcNumberCell = document.createElement("td");
              rcNumberCell.innerHTML = `<input type="text" class="form-control" value=${i} />`;
              row.appendChild(rcNumberCell);

              const fleetVehicleSizeCell = document.createElement("td");
              fleetVehicleSizeCell.innerHTML = `<input type="text" class="form-control" name="description_fixed_assestsl[]" />`;
              row.appendChild(fleetVehicleSizeCell);

              const vehicleDetailCell = document.createElement("td");
              vehicleDetailCell.innerHTML = `<input type="text" class="form-control" name="vehicleOwnedBy[]" />`;
              row.appendChild(vehicleDetailCell);

              tableBody.appendChild(row);
            }
          });
      });
    </script>

